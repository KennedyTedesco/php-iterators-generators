{
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "Iterators\\": "src/"
    }
  },
  "require-dev": {
    "vimeo/psalm": "^3.9",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "scripts": {
    "psalm": [
      "./vendor/bin/psalm --show-info=false"
    ],
    "cs": [
      "./vendor/bin/php-cs-fixer fix --allow-risky yes"
    ]
  }
}
